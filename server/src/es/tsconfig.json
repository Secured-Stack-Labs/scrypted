{
    "compilerOptions": {
        "composite": true,
        "declaration": true,
        "module": "ESNext",
        "target": "esnext",
        "noImplicitAny": true,
        "outDir": "../../dist/es",
        "esModuleInterop": false,
        // skip error: Interface 'WebGL2RenderingContext' incorrectly extends interface 'WebGL2RenderingContextBase'.
        // https://github.com/tensorflow/tfjs/issues/4201
        "skipLibCheck": true,
        "sourceMap": true,
    },
    "include": [
        "./es-eval.ts",
    ],
    "exclude": []
}